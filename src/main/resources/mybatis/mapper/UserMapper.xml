<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ownerkaka.testjdk.mybatis.mapper.UserMapper">
    <select id="getById" parameterType="int" resultType="User">
        select *
        from t_users
        where uid = #{id}
    </select>

    <select id="getByUsername" resultType="com.ownerkaka.testjdk.mybatis.domain.User">
        select *
        from t_users
        where username = #{username}
    </select>
</mapper>